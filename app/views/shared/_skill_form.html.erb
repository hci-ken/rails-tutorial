<%= form_for(@skill) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.text_field :name, class: 'form-control', placeholder: "新しいスキルを入力してください" %>
    <%= f.hidden_field :user_id, :value => @user.id %>
    <%= f.submit "スキルを追加する", class: "btn btn-primary" %>
<% end %>